  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --build-optimizer --optimization --output-hashing=all --extract-licenses --subresource-integrity",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc:build": "compodoc -n FitVerumDocs -t --hideGenerator -p tsconfig.doc.json --customFavicon ./src/favicon.ico --customLogo ./src/assets/images/FitVerumLogo.png -d ./docs/compodoc",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "sassdoc": "sassdoc ./src --config=sassdoc.config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.7",
    "@angular/material": "^15.2.5",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@angular/service-worker": "^15.2.7",
    "@compodoc/compodoc": "^1.1.19",
    "@emailjs/browser": "^3.11.0",
    "@ng-bootstrap/ng-bootstrap": "^14.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.7",
    "angular-calendar": "^0.31.0",
    "bootstrap": "^5.2.3",
    "chart.js": "^3.9.1",
    "chartjs-plugin-annotation": "^2.2.1",
    "date-fns": "^2.29.3",
    "ng2-charts": "^4.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/localize": "^15.2.7",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
